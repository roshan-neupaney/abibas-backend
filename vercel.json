{
  "version": 2,
  "env": {
    "DATABASE_URL": "postgresql://postgres:root@localhost:5432/myDatabase"
  },
  "builds": [{ "src": "dist/main.js", "use": "@vercel/node" }],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/main.js",
      "methods": ["GET", "POST", "PUT", "PATCH", "DELETE"]
    }
  ]
}
